<?xml version="1.0" encoding="utf-8"?>
<xs:schema elementFormDefault="qualified"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:simpleType name="StateType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="normal" />
      <xs:enumeration value="iconic" />
      <xs:enumeration value="withdrawn" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="JustifyType">
    <!--Text justification on a given line.-->
    <xs:restriction base="xs:string">
      <xs:enumeration value="left" />
      <xs:enumeration value="center" />
      <xs:enumeration value="right" />
      <xs:enumeration value="fill" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ReliefType">
    <!--The border style of a widget.-->
    <xs:restriction base="xs:string">
      <xs:enumeration value="raised" />
      <xs:enumeration value="sunken" />
      <xs:enumeration value="flat" />
      <xs:enumeration value="groove" />
      <xs:enumeration value="ridge" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="WrapType">
    <!--Text wrapping options for multi-line text widgets.-->
    <xs:restriction base="xs:string">
      <xs:enumeration value="none" />
      <xs:enumeration value="char" />
      <xs:enumeration value="word" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="StickyType">
    <xs:restriction base="xs:string">
      <xs:pattern value="[nsew]{1,4}" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="BitmapType">
    <!--There are eight built-in, named bitmaps.
  To specify an X bitmap file name, give the full path to the file, preceded with an @, as in "@/bitmap/gumby.bit".-->
    <xs:union memberTypes="NamedBitmaps BitmapFileName" />
  </xs:simpleType>
  <xs:simpleType name="BitmapFileName">
    <!--The eight built-in, named bitmaps.-->
    <xs:restriction base="xs:string">
      <xs:pattern value="@.*" />
    </xs:restriction>

  </xs:simpleType>
  <xs:simpleType name="NamedBitmaps">
    <!--To specify an X bitmap filename, give the full path to the file, preceded with an @, as in "@/usr/contrib/bitmap/gumby.bit".-->
    <xs:restriction base="xs:string">
      <xs:enumeration value="error" />
      <xs:enumeration value="gray25" />
      <xs:enumeration value="gray50" />
      <xs:enumeration value="hourglass" />
      <xs:enumeration value="info" />
      <xs:enumeration value="question" />
      <xs:enumeration value="warning" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="BooleanType">
    <!--You can pass integers 0 or 1 or the strings "yes" or "no".-->
    <xs:union>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="yes" />
          <xs:enumeration value="no" />
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:unsignedByte">
          <xs:minInclusive value="0"/>
          <xs:maxInclusive value="1"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>

  <xs:simpleType name="ColorType">
    <!--Colours can be given as strings representing RGB values or as symbolic names.-->
    <xs:union memberTypes="ColorRGBHex ColorNameTypes" />
  </xs:simpleType>
  <xs:simpleType name="ColorRGBHex">
    <!--Colours can be given as strings representing RGB values in
      4 bit: "#RGB",
      8 bit: "#RRGGBB",
      12 bitâ€ "#RRRGGGBBB", or
      16 bit "#RRRRGGGGBBBB" ranges,
      where R,G,B here represent any legal hex digit..-->
    <xs:union memberTypes="RGB4BitHex RGB8BitHex RGB12BitHex RGB16BitHex" />
  </xs:simpleType>
  <xs:simpleType name="RGB4BitHex">
    <!--RGB values in 4 bit: #RGB.-->
    <xs:restriction base="xs:string">
      <xs:pattern value="#[0-9a-fA-F]{3}" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="RGB8BitHex">
    <!--RGB values in 8 bit: #RRGGBB.-->
    <xs:restriction base="xs:string">
      <xs:pattern value="#[0-9a-fA-F]{6}" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="RGB12BitHex">
    <!--RGB values in 12 bit #RRRGGGBBB".-->
    <xs:restriction base="xs:string">
      <xs:pattern value="#[0-9a-fA-F]{9}" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="RGB16BitHex">
    <!--RGB values in 16 bit #RRRRGGGGBBBB.-->
    <xs:restriction base="xs:string">
      <xs:pattern value="#[0-9a-fA-F]{12}" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ColorNameTypes">
    <!--Internet Standard Color names and Windows THeme based Names.-->
    <xs:union memberTypes="ColorSymbolicNames WindowsThemeColors" />
  </xs:simpleType>
